    "clsx": "^2.1.1",
    "comlink": "^4.4.2",
    "dexie": "^4.2.1",
    "diff": "^8.0.2",
    "fflate": "^0.8.2",
    "immer": "^11.0.1",
    "p-queue": "^9.0.1",
    "radix-ui": "^1.4.3",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.68.0",
    "react-redux": "^9.2.0",
