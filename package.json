{
  "name": "version-control",
  "version": "1.9.27",
  "description": "A local, snapshot-based version control system. Lets you create finite number of versions of any note.",
  "main": "main.js",
  "sideEffects": false,
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev": "node esbuild.config.mjs --watch",
    "build": "node esbuild.config.mjs --production"
  },
  "keywords": [
    "obsidian-md",
    "obsidian-plugin",
    "version-control",
    "history-backup",
    "note-taking",
    "productivity"
  ],
  "author": "Yuichi-Aragi",
  "license": "MIT",
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@reduxjs/toolkit": "^2.8.2",
    "clsx": "^2.1.1",
    "comlink": "^4.4.2",
    "dexie": "^4.2.1",
    "diff": "^8.0.2",
    "fflate": "^0.8.2",
    "immer": "^10.1.1",
    "inversify": "^7.5.4",
    "lodash-es": "^4.17.21",
    "p-queue": "^8.1.0",
    "radix-ui": "^1.4.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.65.0",
    "react-redux": "^9.2.0",
    "react-virtuoso": "^4.13.0",
    "reflect-metadata": "^0.2.2",
    "semver": "^7.7.2",
    "uuid": "^13.0.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.0.12",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/semver": "^7.7.1",
    "@types/uuid": "^10.0.0",
    "esbuild": "^0.25.6",
    "obsidian": "1.10.2-1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  }
}
